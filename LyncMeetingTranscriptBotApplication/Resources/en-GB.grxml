                twelve hundred
                <tag> out._value =  out._value + "1200"; </tag>
              </item>

            </one-of>
          </item>

          <!-- ITN PhoneNumber_UK_AreaCode_01NNN_2_0_X_Double_0-->
          <item>
            one two double
            <tag> out._value = out._value + '12'; </tag>
            <ruleref uri="#Cardinal_0"/>
            <tag> out._value =  out._value + rules.latest()._value; </tag>
          </item>

          <!-- ITN PhoneNumber_UK_AreaCode_01NNN_2_0_X_Single-->
          <item>
            one
            <tag> out._value = out._value + '1'; </tag>

            <ruleref uri="#Cardinal_2_to_9" />
            <tag> out._value =  out._value + rules.latest()._value; </tag>

            <ruleref uri="#Cardinal_0" />
            <tag> out._value = out._value + '0';</tag>

            <ruleref uri="#Cardinal_0_to_9" />
            <tag> out._value = out._value + rules.latest()._value; </tag>
          </item>
        </one-of>
      </item>

      <!-- PhoneNumber_UK_AreaCode_01NNN_2_2_X -->
      <item>
        <!-- X X Y ; where X >= 2, Y >= 0 -->
        <!-- Y X X ; where X >= 2 -->
        <!-- X X X ; where X >= 2 -->
        <!-- Three digit almost works -->
        <!-- Need &gt1 hundreds not need (220), CardinalTens, 01000 is not possible; smallest is 01220-->

        <!-- PhoneNumber_UK_AreaCode_01NNN_2_2_X_Leading1 -->
        one
        <tag> out._value = out._value + '1'; </tag>
        <one-of>

          <!-- PhoneNumber_UK_AreaCode_01NNN_2_2_X_Leading1_Double-->
