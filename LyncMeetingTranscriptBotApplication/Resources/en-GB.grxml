  <!-- 
  PhoneNumber_UK_AreaCode_01NNN._value: string [5 characters]
  This covers larger cities and is followed by an 6 digit subscriber number
  -->

  <!-- ITN PhoneNumber_UK_AreaCode_01NNN-->
  <rule id="PhoneNumber_UK_AreaCode_01NNN" scope="public">
    <example> zero one two one </example>

    <ruleref uri="#Cardinal_0" />
    <tag> out._value = '0'; </tag>

    <!-- ITN PhoneNumber_UK_AreaCode_01NNN_2_0_X_and_2_2_X-->
