              <item>
                <!-- X X -->
                <ruleref uri="#Double_2_to_9" />
                <tag> out._value = out._value + rules.latest()._value; </tag>

                <!-- Y -->
                <ruleref uri="#Cardinal_0_to_9" />
                <tag> out._value = out._value + rules.latest()._value; </tag>
              </item>

              <!-- PhoneNumber_UK_AreaCode_01NNN__2_2_X_Leading1_Double_Y_X_X -->
              <item>
                <!-- Y -->
                <ruleref uri="#Cardinal_2_to_9" />
                <tag> out._value = out._value + rules.latest()._value; </tag>

                <!-- X X -->
                <ruleref uri="#Double_2_to_9" />
                <tag> out._value = out._value + rules.latest()._value; </tag>
              </item>

            </one-of>
          </item>

          <!-- PhoneNumber_UK_AreaCode_01NNN_2_2_X_Leading1_Triple-->
          <item>
            triple two
            <tag> out._value = out._value + '222'; </tag>
          </item>

          <!--PhoneNumber_UK_AreaCode_01NNN_2_2_X_Leading1_Single-->
          <item>
            <!-- ITN Cardinal_2_to_9 -->
            <ruleref uri="#Cardinal_2_to_9" />
            <tag> out._value = out._value + rules.latest()._value; </tag>

            <!-- ITN Cardinal_2_to_9 -->
            <ruleref uri="#Cardinal_2_to_9" />
            <tag> out._value = out._value + rules.latest()._value;</tag>

            <!-- ITN Cardinal_0_to_9 -->
            <ruleref uri="#Cardinal_0_to_9" />
            <tag> out._value = out._value + rules.latest()._value; </tag>
          </item>
        </one-of>

      </item>
    </one-of>
  </rule>
  
  
