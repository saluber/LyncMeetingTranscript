      <item>
        <!-- ITN PhoneNumber_UK_AreaCode_01NNN_2_0_X-->
        <one-of>
          <!-- need 200 to 900 -->
          <!-- need 1200 to 1900 -->
          <!-- need 2 double 0 -->
          <!-- don't need teens, tens, thousands -->

          <!-- ITN PhoneNumber_UK_AreaCode_01NNN_2_0_X_Hundred-->
          <item>
            <one-of>
              <!-- ITN PhoneNumber_UK_AreaCode_01NNN_2_0_X_Hundred_200-->
