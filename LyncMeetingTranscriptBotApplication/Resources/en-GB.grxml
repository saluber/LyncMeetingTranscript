                <!-- ITN Digit_2_to_9_Hundred -->
                <ruleref uri="#Digit_2_to_9_Hundred"/>
                <tag> out._value =  out._value + rules.latest()._value; </tag>
              </item>

              <!-- ITN PhoneNumber_UK_AreaCode_01NNN_2_0_X_Hundred_1200-->
